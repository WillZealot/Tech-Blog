<!--iterate over each post and display any available-->
{{#each post as |posts| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <!-- Render user-specific data with Handlebars.js expressions -->
    <h2>{{posts.title}} {{user_id}}</h2>
  </div>
  <div class="col-md-6">
    <h3>{{posts.content}}</a></h3>
  </div>
</div>
{{/each}}